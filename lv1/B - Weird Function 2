
#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
bool isprime(int x){
  if(x==2)return true;
  else{
    for (int i = 2; i < x; i++)
    {
     if(x%i==0)return false;

    }
    
  }
  return true;
}
string f(int a,string s1,string s2){
   if(a==1)return s1+s2;
   else if(a==0)
   return s2+s1;}
void solve() {
   string s1,s2;
   cin>>s1>>s2;
  cout<<f(1,f(1,f(0,s1,s2)+s1,s2),s1+f(0,s1+s2,f(1,s2,s1)));
    }
    
int main(){
  ios::sync_with_stdio(false);
  cin.tie(nullptr);
   cout.tie(NULL);
 //int t;
  //cin >> t;
  //while(t--)
    solve();
}


