
#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
bool isprime(int x){
  if(x==2)return true;
  else{
    for (int i = 2; i < x; i++)
    {
     if(x%i==0)return false;

    }
    
  }
  return true;
}

void solve() {
char arr[2][100];
int n;
cin>>n;
for (int i = 0; i < 2; i++)
{
    for (int j = 0;j<n; j++)
    {
       cin>>arr[i][j];
    }
    
}
for (int i = 0; i < n; i++)
{
     if(arr[0][i]=='B')arr[0][i]='G';
     if(arr[1][i]=='B')arr[1][i]='G';
     if(arr[0][i]!=arr[1][i]){
        cout<<"NO";
        return;
     }
}
cout<<"YES";
 }
int main(){
  ios::sync_with_stdio(false);
  cin.tie(nullptr);

  int t;
cin >> t;
while(t--){
    solve();
cout<<endl;}

}
