#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
bool isprime(int x){
  if(x==2)return true;
  else{
    for (int i = 2; i < x; i++)
    {
     if(x%i==0)return false;

    }
    
  }
  return true;
}
 void solve(){
int arr[100000];
int n;
cin>>n;
for (int i = 0; i < n; i++)
{
 cin>>arr[i];
}
sort(arr,arr+n);
for(int i = 0; i < n; i++)
{
   cout<<arr[i]<<" ";
}
cout<<endl;
sort(arr,arr+n,greater<int>());
for(int i = 0; i < n; i++)
{
   cout<<arr[i]<<" ";
}


 }
