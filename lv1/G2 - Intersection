#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
bool isprime(int x){
  if(x==2)return true;
  else{
    for (int i = 2; i < x; i++)
    {
     if(x%i==0)return false;

    }
    
  }
  return true;
}
 void solve(){
long long r1,l1,r2,l2;
cin>>r1>>l1>>r2>>l2;
if(r2>l1 || r1>l2)cout<<"-1";
else cout<<max(r1,r2)<<" "<<min(l1,l2);
 }
