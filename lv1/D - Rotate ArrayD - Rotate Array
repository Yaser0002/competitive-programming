
#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
bool isprime(int x){
  if(x==2)return true;
  else{
    for (int i = 2; i < x; i++)
    {
     if(x%i==0)return false;

    }
    
  }
  return true;
}

void solve() {
int n,k;
cin>>n>>k;
int arr[100000];
for (int i = 0; i < n; i++)
{
  cin>>arr[i];
}
reverse(arr+k,arr+n);
for (int i = 0; i < n; i++)
{
  cout<<arr[i]<<" ";
}
}
