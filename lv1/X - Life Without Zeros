
#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
bool isprime(int x){
  if(x==2)return true;
  else{
    for (int i = 2; i < x; i++)
    {
     if(x%i==0)return false;

    }
    
  }
  return true;
}

void solve() {
string a,b;
cin>>a>>b;
long long sum=stoi(a)+stoi(b);
string s=to_string(sum);
while(a.find('0') != string::npos)a.erase(a.find('0'),1);
while(b.find('0') != string::npos)b.erase(b.find('0'),1);
while(s.find('0') != string::npos)s.erase(s.find('0'),1);
if(stoi(a)+stoi(b)==stoi(s))cout<<"YES";
else cout<<"NO";

}
int main(){
  ios::sync_with_stdio(false);
  cin.tie(nullptr);

 // int t;
//    cin >> t;
   //while(t--){
    solve();
  //cout<<endl;}

}
