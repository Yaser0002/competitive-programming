#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
bool isprime(int x){
  if(x==2)return true;
  else{
    for (int i = 2; i < x; i++)
    {
     if(x%i==0)return false;

    }
    
  }
  return true;
}
 void solve(){
   int a,b,c;
   cin>>a>>b>>c;
   if(a+b==c || a+c==b || b+c==a)cout<<"YES";
   else cout<<"NO";
      }
 
int main(){
  ios::sync_with_stdio(false);
  cin.tie(nullptr);

  int t;
cin >> t;
while(t--){
    solve();
cout<<endl;}

}
