#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
bool isprime(int x){
  if(x==2)return true;
  else{
    for (int i = 2; i < x; i++)
    {
     if(x%i==0)return false;

    }
    
  }
  return true;
}
 void solve(){
   long long n;
   int v,f,m,t;
   cin>>v>>f>>m>>t;
   while(true){
    v-=f;
    if(v<0){
      cout<<"F";
      break;
    }
    v-=m;
     if(v<0){
      cout<<"M";
      break;
    }
     v-=t;
     if(v<0){
      cout<<"T";
      break;
   }

      }
    }
